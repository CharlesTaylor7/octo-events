{
  "name": "octo-events",
  "version": "0.0.0",
  "description": "Webhook & API for Github Events",
  "author": "Charles Taylor",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "dev": "dotenv -e .env -- ts-node src/index.ts",
    "pretest": "yarn migrate:test && yarn prisma generate",
    "test": "dotenv -e test.env -- jest",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test-coverage": "yarn test --coverage --collectCoverageFrom=src/**/*.ts",
    "migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "migrate:test": "dotenv -e test.env -- prisma migrate deploy",
    "build": "esbuild src/index.ts --platform=node --bundle --minify --outfile=dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "pg": "^8.11.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.18.17",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
